type Delegation struct {
  dgn &DelegationPayload
  sig &Signature
}

type DelegationPayload struct {
  udv Semver

  iss DID
  aud DID

  exp Integer
  nbf Integer

  sub DID
  can String
  iff [{String : Any}]
}

type Signature union {
  | batch  BatchSig
  | inline Bytes
} representation kinded

type BatchSig struct {
  scp &[Any]
  sig Bytes
}
